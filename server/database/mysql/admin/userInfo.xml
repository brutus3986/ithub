<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="userInfo">

<select id="getUser" >
    <![CDATA[
        select * from  
        users 
        WHERE userid =  #{userid}  
    ]]>	
</select>
    

</mapper>